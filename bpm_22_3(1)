#include <iostream>
#include<vector>
#include <limits>
#include <cmath>
#include <conio.h>

int main() {
		double x = 0.1;
	    double deltax = 0.05;
		double eps = 0.0001;
			const int infinity = std::numeric_limits<int>::max();
			double n = infinity;
			double sum;
			double i = 0;
			std::cin >> x;
			std::cin >> i;
			for (i = 0; i <= n; i++) {
				double an = ((2 * x) * i) / factorial(i);
				while (fabs(an) > eps) {
					sum = sum + an;
					std::cout << sum << std::endl;
				}
			}
				return 0;
	}
